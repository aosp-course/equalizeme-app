<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.equalizeme.ProfileFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/equalizerFragment" />
    </fragment>

    <fragment
        android:id="@+id/equalizerFragment"
        android:name="com.example.equalizeme.EqualizerFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_equalizer" />
</navigation>
